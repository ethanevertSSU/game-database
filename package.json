{
  "name": "game-database",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist out .cache node_modules",
    "check:updates": "ncu",
    "db:viewer": "npx prisma studio",
    "db:migrate:createonly": "dotenv -e .env -- npx prisma migrate dev --create-only",
    "db:migrate:dev": "dotenv -e .env -- npx prisma migrate dev",
    "db:generate": "dotenv -e .env -- npx prisma generate"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.4",
    "@mui/material": "^6.4.4",
    "@mui/styled-engine": "^6.4.3",
    "@prisma/client": "^6.3.1",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-hover-card": "^1.1.6",
    "bcrypt": "^5.1.1",
    "better-auth": "^1.1.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "lucide-react": "^0.479.0",
    "next": "15.1.7",
    "next-themes": "^0.4.4",
    "prisma": "^6.3.1",
    "radix-ui": "^1.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.1",
    "steam-lightweight-openid": "^1.0.0",
    "swr": "^2.3.3",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "vercel": "^41.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "npm-check-updates": "^17.1.14",
    "postcss": "^8",
    "prettier": "3.5.3",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
